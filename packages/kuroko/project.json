{
  "name": "kuroko",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{workspaceRoot}/dist/packages/stencil-pkg"],
      "options": {
        "command": "pnpm build",
        "cwd": "packages/stencil-pkg"
      }
    },
    "dev": {
      "executor": "nx:run-commands",
      "outputs": ["{workspaceRoot}/dist/packages/stencil-pkg"],
      "options": {
        "command": "pnpm start",
        "cwd": "packages/stencil-pkg"
      }
    }
  },
  "tags": ["stenciljs", "webcomponents"]
}
