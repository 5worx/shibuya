{
  "name": "shibuya",
  "version": "0.0.1",
  "description": "Framework Agnostic Development System",
  "type": "module",
  "scripts": {
    "forge": "node ./shibuya.js forge --apps=angular-app,rust-api",
    "dev": "node ./shibuya.js dev --apps=angular-app,rust-api",
    "start": "node ./shibuya.js start --apps=angular-app,rust-api",
    "stop": "node ./shibuya.js stop --apps=angular-app,rust-api",
    "melt": "node ./shibuya.js melt --apps=angular-app,rust-api",
    "helper:check": "node ./helper/shibuya-check.mjs && node ./helper/sensei.mjs",
    "helper:clean": "node ./helper/clean-all.mjs",
    "helper:export-realms": "node ./helper/export-keycloak-realm.mjs",
    "mokuroku:view": "node ./helper/mokuroku.mjs view",
    "mokuroku:adr": "node ./helper/mokuroku.mjs adr",
    "suido:urls": "node ./helper/suido/suido.js --urls"
  },
  "keywords": [
    "framework",
    "agnostic",
    "developer",
    "system",
    "shibuya"
  ],
  "author": "",
  "license": "MIT",
  "packageManager": "pnpm@10.28.1+sha512.7d7dbbca9e99447b7c3bf7a73286afaaf6be99251eb9498baefa7d406892f67b879adb3a1d7e687fc4ccc1a388c7175fbaae567a26ab44d1067b54fcb0d6a316",
  "engines": {
    "node": ">=24.11.1"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "nx": "^22.4.0"
  },
  "dependencies": {
    "js-yaml": "^4.1.1"
  }
}
