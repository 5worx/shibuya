{
  "name": "shibuya",
  "version": "0.0.1",
  "description": "Framework Agnostic Development System",
  "type": "module",
  "scripts": {
    "postinstall": "node ./shibuya.js install",
    "forge": "node ./shibuya.js forge && node ./shibuya/sensei.mjs",
    "dev": "node ./shibuya.js dev",
    "start": "node ./shibuya.js start",
    "stop": "node ./shibuya.js stop",
    "melt": "node ./shibuya.js melt",
    "suido:urls": "node ./shibuya/suido/suido.js --urls",
    "shibuya:check": "node ./shibuya/shibuya-check.mjs && node ./shibuya/sensei.mjs",
    "shibuya:clean:all": "node ./shibuya/clean-all.mjs --all",
    "shibuya:clean:builds": "node ./shibuya/clean-all.mjs --builds",
    "shibuya:clean:dependencies": "node ./shibuya/clean-all.mjs --dependencies",
    "shibuya:clean:infra": "node ./shibuya/clean-all.mjs --infrastructure",
    "mokuroku:view": "node ./shibuya/sensei.mjs && node ./shibuya/mokuroku-viewer.mjs",
    "mokuroku:retro": "node ./shibuya/mokuroku.mjs retro",
    "mokuroku:notes": "node ./shibuya/mokuroku.mjs notes",
    "mokuroku:lock": "node ./shibuya/mokuroku.mjs lock",
    "mokuroku:unlock": "node ./shibuya/mokuroku.mjs unlock",
    "bug:link": "node shibuya/bug-link.mjs",
    "bug:add": "node shibuya/bug-add.mjs",
    "keycloak:export-realms": "nx export-realm kpn-keycloak"
  },
  "keywords": [
    "framework",
    "agnostic",
    "developer",
    "system",
    "shibuya"
  ],
  "author": "",
  "license": "MIT",
  "packageManager": "pnpm@10.28.1+sha512.7d7dbbca9e99447b7c3bf7a73286afaaf6be99251eb9498baefa7d406892f67b879adb3a1d7e687fc4ccc1a388c7175fbaae567a26ab44d1067b54fcb0d6a316",
  "engines": {
    "node": ">=24.11.1"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "express": "^5.2.1",
    "marked": "^17.0.1",
    "nx": "^22.4.0",
    "open": "^11.0.0"
  },
  "dependencies": {
    "js-yaml": "^4.1.1"
  }
}
